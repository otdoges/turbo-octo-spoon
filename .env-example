# API Keys - Never commit actual keys to the repository
# Copy this file to .env and add your actual API keys

# Frontend environment variables (prefixed with VITE_)
# These are exposed to the browser

# API URL for backend connection (must include http:// prefix)
VITE_API_URL=http://localhost:3002

# AI Service API Keys
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here
VITE_GITHUB_TOKEN=your_github_token_here

# Authentication (Clerk)
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# Database (Supabase)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Database (Convex)
VITE_CONVEX_URL=your_convex_url_here

# Backend and Serverless environment variables
# These are NOT exposed to the browser

# Screenshot One API key for website screenshots
# Get yours at https://screenshot-one.com/
SCREENSHOT_API_KEY=your_screenshot_one_api_key_here

# Cloudinary configuration for image uploads in serverless functions
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# File storage configuration
FILE_ENCRYPTION_KEY=your_encryption_key_here
MAX_FILE_SIZE_MB=10
FILE_RETENTION_MINUTES=30

# Server configuration
PORT=3002

# Environment
NODE_ENV=development 